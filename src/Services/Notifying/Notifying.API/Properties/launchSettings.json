{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:27379",
      "sslPort": 44347
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionString": "Server=tcp:127.0.0.1,5433;Initial Catalog=Notifications.NotificationDB;User Id=sa;Password=Pass@word",
        "SubscriberConnectionString": "Server=tcp:127.0.0.1,5433;Initial Catalog=Notifications.SubscriberDB;User Id=sa;Password=Pass@word",
        "Serilog:LogstashgUrl": "http://locahost:8080",
        "EventBusConnection": "localhost",
        "Serilog:SeqServerUrl": "http://locahost:5340"
      }
    },
    "Notifications.API": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5104/",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/weatherforecast",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}